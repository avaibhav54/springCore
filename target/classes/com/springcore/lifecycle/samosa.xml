<?xml version="1.0" encoding="UTF-8"?>



<beans 

 xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
<context:annotation-config/>

<bean class="com.springcore.lifecycle.samosa" name="sa" init-method="init" destroy-method="destroy">
<property name="price" value="6"/>
 	
</bean>


<bean class="com.springcore.lifecycle.pepsi" name="pa">
<property name="price" value="60"/>

</bean>

<bean class="com.springcore.lifecycle.khsta" name="ka">
<property name="price" value="10"/>

</bean>





</beans>